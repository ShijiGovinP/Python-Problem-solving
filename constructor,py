class phone:
    def __init__(self):
        self.model=""
        self.ram=""
        self.warranty=""
        self.price=0
    def display(self):
         print("The Model is:",self.model)
         print("RAM is:",self.ram)
         print("warranty of mobile:",self.warranty)
         print("PRICE IS:",self.price)   
object=phone()
object.model="realme"
object.ram="8gb"
object.warranty="2 years"
object.price=40000

object.display()
